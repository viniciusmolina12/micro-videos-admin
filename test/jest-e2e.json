{
  "testRegex": ".e2e-spec.ts$",
  "clearMocks": true,
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(t|j)s$": "@swc/jest"
  },
  "coverageProvider": "v8",
  "coverageReporters": ["text", "lcov"],
  "coverageDirectory": "coverage",
  "setupFilesAfterEnv": ["./jest-setup.ts"]
}
